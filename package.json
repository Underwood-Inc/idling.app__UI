    "dotenv": "^16.4.5",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "jotai": "^2.12.5",
    "lorem-ipsum": "^2.0.8",
    "next": "^14.2.15",
    "next-auth": "^5.0.0-beta.22",
    "pg": "8.10.0",
    "postgres": "3.4.4",
    "react": "^19.0.0-rc-e56f4ae3-20240830",
    "react-dom": "^19.0.0-rc-e56f4ae3-20240830",
    "react-icons": "^5.3.0",
