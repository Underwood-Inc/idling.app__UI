{
  "name": "eslint-plugin-custom-rules",
  "version": "1.0.0",
  "description": "Custom ESLint rules for idling.app",
  "main": "dist/index.js",
  "type": "commonjs",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "test": "jest --config jest.config.cjs",
    "test:watch": "jest --config jest.config.cjs --watch",
    "test:coverage": "jest --config jest.config.cjs --coverage",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf dist",
    "prepare": "npm run clean && npm run build",
    "postinstall": "chmod -R 755 dist || true"
  },
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin"
  ],
  "author": "idling.app",
  "license": "ISC",
  "peerDependencies": {
    "@typescript-eslint/parser": "^6.0.0",
    "@typescript-eslint/utils": "^6.0.0",
    "eslint": "^8.0.0",
    "typescript": "^5.0.0"
  },
  "devDependencies": {
    "@types/eslint": "^8.0.0",
    "@types/jest": "^29.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@typescript-eslint/utils": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "rimraf": "^5.0.0",
    "ts-jest": "^29.0.0",
    "typescript": "^5.0.0"
  }
}
